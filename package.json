{
  "name": "cc",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "codecov": "^3.5.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-scripts": "2.1.8",
    "react-select": "^3.0.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "coverage": "react-scripts test --env=jsdom --watchAll=false --coverage"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "coverageReporters": [
    "text"
  ],
  "snapshotSerializers": [
    "my-serializer-module"
  ],
  "coverageThreshold": {
    "global": {
      "branches": 90,
      "functions": 90,
      "lines": 90,
      "statements": 90
    }
  },
  "devDependencies": {
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "jest-cli": "^24.9.0"
  }
}
